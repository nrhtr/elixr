#!./parse_dump
object $root;
method foo()
{
    var i = ["hey", 2, 3]

    "i = " show; i string showln

    "i[0] = " show; i at(0) showln
    
    # list are mutable and
    # :append mutates
    i append(:doggy)
    "i = " show; i string showln

    # :append also returns the 'self' reference so
    # you can this
    var d = []
    d append(42) append(7)
    d string showln
}

#!./parse_dump
object $root;
method foo()
{
    var p16 = 1.0
    var pi = 0.0
    var precision = 10

    var k = 0
    while (k <= precision)
    {
        pi = pi + ( 1.0/p16 * (4.0/(8*k + 1) - 2.0/(8*k + 4) - 1.0/(8*k + 5) - 1.0/(8*k+6)))
        p16 = p16 * 16
        k = k + 1
    }
    
    pi string showln
}
